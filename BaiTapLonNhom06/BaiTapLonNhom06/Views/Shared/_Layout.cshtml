<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BaiTapLonNhom06</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/BaiTapLonNhom06.styles.css" asp-append-version="true" />
</head>

@{
    string menuTextColor = "text-white";
}
<body>
    <div class="container-fluid" style="background:Linear-gradient(to right,#28340B,#197696);">
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm  border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand @menuTextColor" asp-area="" asp-controller="Home" asp-action="Index">BaiTapLonNhom06</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link @menuTextColor" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link @menuTextColor" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                          <a class="nav-link @menuTextColor" asp-area="" asp-controller="Client" asp-action="Index">DSClient</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link @menuTextColor" asp-area="" asp-controller="Client" asp-action="Create">NhapClient</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link @menuTextColor" asp-area="" asp-controller="Employee" asp-action="Index">DSEmployee</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link @menuTextColor" asp-area="" asp-controller="Employee" asp-action="Create">NhapEmployee</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link @menuTextColor" asp-area="" asp-controller="Service" asp-action="Index">DSService</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link @menuTextColor" asp-area="" asp-controller="Service" asp-action="Create">NhapService </a>
                        </li>
                         <li class="nav-item">
                            <a class="nav-link @menuTextColor" asp-area="" asp-controller="Phong" asp-action="Index">Phong</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
 </div>
    <div class="container">
        <main role="main" class="pb-3">


            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2022 - BaiTapLonNhom06 - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
